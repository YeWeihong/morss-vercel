{
  "version": 2,
  "name": "morss",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "www/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/www/(.*)",
      "dest": "/www/$1"
    },
    {
      "src": "/(index\\.html|xpath-selector\\.html|.*\\.(css|js|svg|xsl|md))",
      "dest": "/www/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "MAX_ITEM": "30",
    "MAX_TIME": "20",
    "TIMEOUT": "8",
    "LIM_TIME": "25",
    "LIM_ITEM": "50"
  }
}
